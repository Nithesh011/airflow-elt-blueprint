{
  "name": "Airflow ELT Blueprint",
  "image": "python:3.9-slim",
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
  },
  "postCreateCommand": "pip install 'astro-sdk-python>=1.6.0' && astro dev init",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "portsAttributes": {
    "8080": { "label": "Airflow UI", "onAutoForward": "openBrowser" },
    "8501": { "label": "Streamlit", "onAutoForward": "openBrowser" }
  },
  "remoteUser": "root"
}
